package Mid;

public class ArraySumProduct {

	public static void main(String[] args) {
		int Array[]= {1,2,3,4,5,6,7,8,9};
		int len=Array.length;
		int sum=0;
		int product=1;
		
		for(int i=0;i<len;i++) {
			sum=sum+Array[i];
			product=product*Array[i];
		}
		System.out.println("Sum : "+sum);
		System.out.println("Product: "+product);
	}

}
